<h2> New Study Session </h2>
<%= form_for(@study_session) do |f| %>
  <div> <%= f.label :label%>  <%= f.text_field :label, :autofocus => true %>
  </div>

  <div><%= f.label :location%>  <%= f.text_field :location%></div>

  <div><%= f.label :subject %>  <%= f.select :subject_id, Subject.all.collect {|s| [s.label, s.id]}{include_blank:true} %></div>

  <br>

  <div><%= f.submit "Create Session", :class => "btn btn-primary" %></div>
<% end %>
